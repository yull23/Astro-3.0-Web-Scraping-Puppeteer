---
import Header from "@/components/general/Header.astro";
import LaunchGrid from "@/components/specific/LaunchGrid.astro";
import Layout from "@/layouts/Layout.astro";
import { getLaunchesSpaceX } from "@/services/api-spaceX";

const launches = await getLaunchesSpaceX();
const failures = launches.filter((launch) => launch.failures);
const successful = launches.filter((launch) => !launch.failures);
---

<Layout title="Welcome to Astro.">
  <Header />
  <LaunchGrid launches={failures} />
</Layout>
